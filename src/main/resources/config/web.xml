<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2.2.dtd">
<web-app>
	<servlet>
		<servlet-name>
           htmlTest
        </servlet-name>
		<servlet-class>
           com.hp.hpl.deliTest.TestCCPPServlet
        </servlet-class>
	</servlet>
	<servlet>
		<servlet-name>
           wmlTest
        </servlet-name>
		<servlet-class>
           com.hp.hpl.deliTest.TestCCPPServletWML
        </servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>
          htmlTest
       </servlet-name>
		<url-pattern>
         /html/*
       </url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>
          wmlTest
       </servlet-name>
		<url-pattern>
         /wml/*
       </url-pattern>
	</servlet-mapping>
</web-app>
