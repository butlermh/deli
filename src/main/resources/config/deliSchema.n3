@prefix deli: <http://delicon.sourceforge.net/schema#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .

deli:validatorOutput
	rdf:type rdf:Property;
	rdfs:label "validatorOutput"@en;
	rdfs:comment "Why the profile was invalid"@en.

deli:validatorResult
	rdf:type rdf:Property;
	rdfs:label "validatorResult"@en;
	rdfs:comment "The result of the validator"@en.
	
deli:Valid
	rdf:type rdfs:Class;
	rdfs:comment "Valid profile"@en.

deli:Unretrievable
	rdf:type rdfs:Class;
	rdfs:comment "Unretrievable"@en.
	
deli:Invalid
	rdf:type rdfs:Class;
	rdfs:comment "Invalid"@en.

deli:profileSuppliedByVendor
	rdf:type	rdf:Property;
	rdfs:label  "profileSuppliedByVendor"@en ;
	rdfs:comment "Profile supplied by vendor"@en .

deli:deviceName
	rdf:type	rdf:Property;
	rdfs:label	"deviceName"@en ;
	rdfs:comment	"The name of the device"@en .

deli:manufacturedBy
	rdf:type	rdf:Property;
	rdfs:label      "manufacturedBy"@en ;
	rdfs:comment	"The manufacturer who created the device described by the profile"@en .

deli:uaprofUri
	rdf:type	rdf:Property;
	rdfs:label	"uaprofUri"@en ;
	rdfs:comment	"The URI of the UAProf profile"@en . 

deli:provider
	rdf:type	rdf:Property;
	rdfs:label	"provider"@en ;
	rdfs:comment	"The organization who provided the profile"@en .

deli:useragent
	rdf:type	rdf:Property;
	rdfs:label	"useragent";
	rdfs:comment	"The useragent of the device"@en .

deli:file
	rdf:type	rdf:Property;
	rdfs:label	"file";
	rdfs:comment	"A file containing the profile"@en . 

deli:release
	rdf:type	rdf:Property;
	rdfs:label	"Release";
	rdfs:comment 	"The release of this profile"@en .
	
deli:supportsUAProf
	rdf:type	rdf:Property;
	rdfs:label	"supportsUAProf";
	rdfs:comment	"Does the device support UAProf?"@en . 

deli:Profile
	rdf:type	rdfs:Class;
	rdfs:label	"A profile"@en .
	
deli:Manufacturer
	rdf:type	rdfs:Class;
	rdfs:label "A manufacturer"@en .

deli:localProfilesFile 
	rdf:type	rdf:Property;
	rdfs:label	"localProfilesFile";
	rdfs:comment "the file containing details of local profiles used to override devices with no profiles"@en .

deli:localProfilesPath 
	rdf:type	rdf:Property;
	rdfs:label	"localProfilesPath";
	rdfs:comment "the path to the local profiles"@en .
	
