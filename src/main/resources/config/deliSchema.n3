deli:rdfsUri
	rdf:type	rdf:Property;
	rdfs:label	"rdfsURI";
	rdfs:comment "the RDF schema URI"@en . 
	
deli:NamespaceDefinition
	rdf:type	rdfs:Class;
	rdfs:label "nameSpaceDefintion" ;
	rdfs:comment	"A namespace definition"@en .

deli:uri
	rdf:type	rdf:Property;
	rdfs:label	"URI";
	rdfs:comment "the URI of a namespace"@en .  

deli:aliasUri
	rdf:type	rdf:Property;
	rdfs:label "alias URI"@en ;
	rdfs:comment "the alias URI of a namespace"@en .
   
deli:schemaVocabularyFile
	rdf:type	rdf:Property;
	rdfs:label "schemaVocabularyFile"@en ;
	rdfs:comment "the schema vocabulary file"@en .  

deli:datatypeUri
	rdf:type	rdf:Property;
	rdfs:label "datatypeUri"@en ;
	rdfs:comment "the datatype uri"@en .
	
deli:datatypeFile
	rdf:type	rdf:Property;
	rdfs:label "datatypeFile"@en ;
	rdfs:comment "the file containing the datatype file"@en .	

deli:profileDiffDigestVerification
	rdf:type	rdf:Property ;
	rdfs:label "profileDiffDigestVerification"@en ;
	rdfs:comment "Profile Diff Digest Verification"@en .

deli:preferLocalOverRemoteProfiles
	rdf:type	rdf:Property ;
	rdfs:label "preferLocalOverRemoteProfiles"@en ;
	rdfs:comment "Prefer local profiles over remote profiles"@en .
	
deli:debugRequestHeaders
	rdf:type	rdf:Property ;
	rdfs:label "debugRequestHeaders"@en;
	rdfs:comment "debug request headers"@en .
	
deli:refreshStaleProfiles
	rdf:type	rdf:Property ;
	rdfs:label "refreshStaleProfiles"@en;
	rdfs:comment "Refresh stale profiles"@en .
	
deli:useLocalProfilesIfNoCCPP
	rdf:type	rdf:Property ;
	rdfs:label "useLocalProfilesIfNoCCPP"@en;
	rdfs:comment "use local profiles if no CC/PP information in request"@en .				

deli:normaliseWhitespaceInProfileDiff
	rdf:type	rdf:Property ;
	rdfs:label "normaliseWhitespaceInProfileDiff"@en;
	rdfs:comment "normalize whitespaces in the profile diff"@en .
	
deli:processUnconfiguredNamespaces
	rdf:type	rdf:Property ;
	rdfs:label "processUnconfiguredNamespaces"@en;
	rdfs:comment "process unconfigured namespaces"@en .
	
deli:cacheReferenceProfiles
	rdf:type	rdf:Property ;
	rdfs:label "cacheReferenceProfiles"@en;
	rdfs:comment "cache reference profiles"@en .
				
deli:datatypeConfigFile
	rdf:type	rdf:Property ;
	rdfs:label "datatypeConfigFile"@en;
	rdfs:comment "file containing datatype configuration"@en .
	
deli:componentProperty
	rdf:type	rdf:Property ;
	rdfs:label "componentProperty"@en;
	rdfs:comment "the property to use for components"@en .		
	
deli:maxCachedProfileLifetime
	rdf:type	rdf:Property ;
	rdfs:label "maxCachedProfileLifetime"@en;
	rdfs:comment "the maximum lifetime for a profile in the cache"@en .
	
deli:maxCacheSize
	rdf:type	rdf:Property ;
	rdfs:label "maxCacheSize"@en ;
	rdfs:comment "the maximum cache size"@en .