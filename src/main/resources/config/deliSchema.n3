#	rdfs:comment "print the profile before performing a merge"@en .
	
deli:processUndefinedAttributes
	rdf:type	rdf:Property;
	rdfs:label	"processUnderfinedAttributes";
	rdfs:comment "whether to process properties that have not been defined in a schema"@en .
		
deli:datatypeValidationOn 
	rdf:type	rdf:Property;
	rdfs:label	"datatypeValidationOn";
	rdfs:comment "whether to use datatype validation or not"@en .
	
